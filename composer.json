{
    "name": "dmt-software/eu-vat-validation",
    "description": "VIES VAT Validation client",
    "keywords": [
        "vat", "vies", "tax", "validate", "trade", "company", "service"
    ],
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Bas de Mes",
            "email": "bas@dmt-software.nl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "dmt-software/jms-soap-serializer": "^2.1",
        "dmt-software/command-bus-validator": "^1.0",
        "dmt-software/http-client-middleware": "^1.0 | ^2.0"
    },
    "autoload": {
        "psr-4": {
            "DMT\\VatServiceEu\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 | ^9.5",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload-dev": {
        "psr-4": {
            "DMT\\Test\\VatServiceEu\\": "tests/"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "for PSR-18 client implementation",
        "guzzlehttp/psr7": "for PSR-17 message factor implementation"
    }
}
